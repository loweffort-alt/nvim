{
  //Esto es para el prettier
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "html",
    "javascript",
    "go",
    "javascriptreact",
    "astro"
  ],
  "eslint.autoFixOnSave": "true",
  "snippets.ultisnips.pythonPrompt": false,
  "eslint.probe":["javascript","javascriptreact","typescript","typescriptreact","html","astro","markdown, json"],
  "snippets.userSnippetsDirectory": "~/AppData/Local/nvim/snippets/mysnippets",
  "snippets.textmateSnippetsRoots": ["~/AppData/Local/nvim/snippets/textmate"],
  "snippets.snipmate.author": "Alex Farfan Navarro"
}

